BasedOnStyle: LLVM
IncludeBlocks: Regroup
IncludeCategories:
  - Regex:           '^"genpybind/'
    Priority:        2
  - Regex:           '^<(llvm|clang)/'
    Priority:        3
  - Regex:           '^<(gtest|gmock)/'
    Priority:        4
